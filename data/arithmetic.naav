main {
    num num1 = 20;
    num num2 = 5;

    # Addition
    num sum = num1 + num2;
    print("Sum: " + sum);

    # Subtraction
    num difference = num1 - num2;
    print("Difference: " + difference);

    # Multiplication
    num product = num1 * num2;
    print("Product: " + product);

    # Division
    num quotient = (num2 != 0) ? (num1 / num2) : 0;
    print("Quotient: " + quotient);
}
